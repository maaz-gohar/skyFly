{
  "expo": {
    "name": "skyFly",
    "slug": "skyFly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/plane1.png",
    "scheme": "skyfly",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/plane1.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A6FFF"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#4A6FFF",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/plane1.png",
        "backgroundColor": "#4A6FFF"
      },
      "splash": {
        "image": "./assets/images/plane1.png",
        "resizeMode": "contain",
        "backgroundColor": "#4A6FFF",
        "tabletImage": "./assets/images/plane1.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/plane1.png",
        "backgroundColor": "#4A6FFF"
      },
      "splash": {
        "image": "./assets/images/plane1.png",
        "resizeMode": "contain",
        "backgroundColor": "#4A6FFF",
        "mdpi": "./assets/images/plane1.png",
        "hdpi": "./assets/images/plane1.png",
        "xhdpi": "./assets/images/plane1.png",
        "xxhdpi": "./assets/images/plane1.png",
        "xxxhdpi": "./assets/images/plane1.png"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/plane1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/plane1.png",
          "backgroundColor": "#4A6FFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}